{
  "name": "react-ace",
  "version": "2.6.0",
  "description": "A react component for Ace Editor",
  "main": "lib/ace.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "echo \"Temporarily remove\"",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/ace.js dist/react-ace.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/ace.js dist/react-ace.min.js --config webpack.config.production.js",
    "build:example": "cd example && npm install",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "install": "npm run build:lib",
    "check": "npm run lint",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "James Hrisho",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.2.0",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.0",
    "gulp-ignore": "^2.0.1",
    "gulp-jest-iojs": "^1.0.2",
    "gulp-jshint": "^1.11.2",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "^1.4.1",
    "react-addons-test-utils": "^0.14.3",
    "vinyl-source-stream": "^1.1.0",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "keywords": [
    "ace",
    "ace editor",
    "react-component",
    "react",
    "brace"
  ],
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "dependencies": {
    "brace": "^0.5.1"
  },
  "peerDependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/patope/react-ace.git"
  }
}
